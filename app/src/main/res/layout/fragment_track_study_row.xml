<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/ll_track_study"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_track_study_tap"
        style="@style/AppTheme.ListviewButton"
        android:text="@string/label_tempo_tap2"/>

    <TextView
        android:id="@+id/tv_track_study_study"
        style="@style/AppTheme.ListviewButton"
        android:text="@string/label_study_off"/>

    <TextView
        android:id="@+id/tv_track_study_practice"
        style="@style/AppTheme.ListviewButton"
        android:text="@string/label_practice"/>

</LinearLayout>