            android:tag="iv18"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis19"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv19"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_editor_tempo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/button_m20"
            style="@style/AppTheme.TempoButton"
            android:layout_toLeftOf="@+id/button_m5"
            android:text="@string/label_m20"/>

        <Button
            android:id="@+id/button_m5"
            style="@style/AppTheme.TempoButton"
            android:layout_toLeftOf="@+id/button_m1"
            android:text="@string/label_m5"/>

        <Button
            android:id="@+id/button_m1"
            style="@style/AppTheme.TempoButton"
            android:layout_toLeftOf="@+id/tv_editor_tempo"
            android:text="@string/label_m1"/>

        <TextView
            android:id="@+id/tv_editor_tempo"
            android:layout_width="@dimen/incdec_width"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@color/color_emphasis_low"
            android:gravity="center_vertical|center_horizontal"
            android:text="@string/hw"
            android:textColor="@color/color_text"
            android:textSize="@dimen/textsize_timescale"/>

        <Button
            android:id="@+id/button_p1"
            style="@style/AppTheme.TempoButton"
            android:layout_toRightOf="@+id/tv_editor_tempo"
            android:text="@string/label_p1"/>


        <Button
            android:id="@+id/button_p5"
            style="@style/AppTheme.TempoButton"
            android:layout_toRightOf="@+id/button_p5"
            android:text="@string/label_p5"/>

        <Button
            android:id="@+id/button_p20"
            style="@style/AppTheme.TempoButton"
            android:layout_toRightOf="@+id/button_p20"
            android:text="@string/label_p20"/>

    </LinearLayout>

    <SeekBar
        android:id="@+id/sb_tempo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tv_editor_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_text"
        android:textSize="@dimen/textsize_row"/>

    <LinearLayout
        android:id="@+id/ll_editor_spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/label_timesignature"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_timesig"/>

        <Spinner
            android:id="@+id/spinnerBeat"
            android:layout_width="50dp"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_timesigsep"/>

        <Spinner
            android:id="@+id/spinnerTime"
            android:layout_width="40dp"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/tvSub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_timesigsep"/>

        <Spinner
            android:id="@+id/spinnerSub"
            android:layout_width="80dp"
            android:layout_height="wrap_content"/>

        <Button
            android:id="@+id/tv_editor_buttonTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_title"/>

        <TextView
            android:id="@+id/tv_editor_currentbeat"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:background="#FFFFFF"
            android:gravity="center_horizontal"
            android:textSize="@dimen/textsize_row"
            android:visibility="gone"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_editor_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/buttonTempoTap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_tempo_tap2"/>

        <Button
            android:id="@+id/buttonStudy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_study_off"/>

        <Button
            android:id="@+id/buttonPractice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_practice"/>
    </LinearLayout>

    <ListView
        android:id="@+id/track_listView"
        android:choiceMode="multipleChoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>
