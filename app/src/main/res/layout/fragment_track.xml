            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv00"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis1"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv01"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis2"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv02"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis3"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv03"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis4"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv04"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis5"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv05"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis6"
