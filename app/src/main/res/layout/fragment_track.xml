        <ImageView
            android:id="@+id/iv_ed_editor_emphasis9"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv09"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_ed_editor_emphasis1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="3dp">

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis10"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv10"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis11"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv11"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis12"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv12"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis13"
