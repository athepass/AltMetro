            android:tag="iv18"/>

        <ImageView
            android:id="@+id/iv_ed_editor_emphasis19"
            style="@style/AppTheme.EmphasisEditor"
            android:tag="iv19"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_ed_player_emphasis0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="3dp"
        android:visibility="gone">

        <ImageView
            android:id="@+id/iv_ed_player_emphasis0"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv00"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis1"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv01"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis2"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv02"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis3"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv03"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis4"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv04"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis5"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv05"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis6"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv06"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis7"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv07"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis8"
