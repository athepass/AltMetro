    <LinearLayout
        android:id="@+id/ll_ed_player_emphasis1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="3dp"
        android:visibility="gone">

        <ImageView
            android:id="@+id/iv_ed_player_emphasis10"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv10"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis11"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv11"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis12"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv12"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis13"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv13"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis14"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv14"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis15"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv15"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis16"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv16"/>

        <ImageView
            android:id="@+id/iv_ed_player_emphasis17"
            style="@style/AppTheme.EmphasisPlayer"
            android:tag="iv17"/>

